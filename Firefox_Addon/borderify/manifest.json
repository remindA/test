{
    "manifest_version": 2,
    "name": "Norderify",
    "version": "1..0",
    "description": "Add a red border to all webpages matching mozilla.org",
    "icons": {
        "48": "icons/border-48.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.baidu.com/*"],
            "js": ["borderify.js"]
        }
    ]
}
